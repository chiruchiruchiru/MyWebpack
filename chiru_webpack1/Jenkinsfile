pipeline { // Declarative pipelineであることを宣言する
  environment {
    SERVICE_CREDS = credentials('github-credentials')
  }
  agent any
  stages {
    stage("get resource") {
      steps {
        git 'https://github.com/chiruchiruchiru/MyWebpack.git'
      }
    }
    stage('Run test') {
      steps {
        sh "pwd"
        sh "ls -a"
        sh "mkdir -p ./rrr/ttt"
      }
    }
    stage('copy') {
      steps {
        copyArtifacts(projectName: 'simplePipeline');
      }
    }
  }
}